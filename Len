#include <iostream>


using namespace std;

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int integers[] = {5, 4, 3, 2, 1};
	int num = sizeof(integers) / sizeof(integers[0]);
	
	cout<< num << endl;
	return 0;
}
